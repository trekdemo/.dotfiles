# Install config files

  $ stow dev executables git tmux xdg_config terminfo

# Install italics

  # Linux
  $ tic -x ~/.terminfo ~/.terminfo/src/tmux.terminfo
  $ tic -x ~/.terminfo ~/.terminfo/src/tmux-256color.terminfo
  $ tic -x ~/.terminfo ~/.terminfo/src/xterm-256color.terminfo

  # MacOS
  $ tic -c ~/.terminfo ~/.terminfo/src/tmux.terminfo
  $ tic -c ~/.terminfo ~/.terminfo/src/tmux-256color.terminfo
  $ tic -c ~/.terminfo ~/.terminfo/src/xterm-256color.terminfo
