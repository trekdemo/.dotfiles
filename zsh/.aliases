alias vim="mvim -v"
alias pryr='pry -r ./config/environment.rb'
alias tm='TERM=screen-256color-bce tmux -u2 -S ~/.tmux-server'
alias collapse="sed -e 's/  */ /g'"
alias cuts="cut -d' '"
alias clc="./get-last-commit-url.py | pbc"
alias count="sort | uniq -c | sort -n"
alias rrg="rake routes | grep"
alias :q="exit"
alias tl="tail -f log/development.log"

alias fos='foreman start'

# PSQL commands
alias psql-connect-development='psql `basename $PWD`_development'
alias psql-connect-test='psql `basename $PWD`_test'
alias psql-connect='psql-connect-development'

# Bundler aliases
alias be="bundle exec"
alias bi="bundle install"
alias bl="bundle list"
alias bp="bundle package"
alias bu="bundle update"
